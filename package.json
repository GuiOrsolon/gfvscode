{
  "name": "gfvscode",
  "displayName": "Grails",
  "description": "Develop Grails Applications easy on VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gfvscode.createApp",
        "category": "Grails",
        "title": "Create App"
      },
      {
        "command": "gfvscode.runnApp",
        "category": "Grails",
        "title": "Run App"
      },
      {
        "command": "gfvscode.stopApp",
        "category": "Grails",
        "title": "Stop App"
      },
      {
        "command": "gfvscode.createDomainClass",
        "category": "Grails",
        "title": "Create Domain Class"
      },
      {
        "command": "gfvscode.createController",
        "category": "Grails",
        "title": "Create Controller"
      },
      {
        "command": "gfvscode.createService",
        "category": "Grails",
        "title": "Create Service"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "properties-reader": "^2.2.0"
  }
}
